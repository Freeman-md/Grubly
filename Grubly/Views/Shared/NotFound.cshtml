@model string

@{
ViewData["Title"] = "Not Found";
}

<div class="flex flex-col items-center justify-center max-w-md mx-auto">
    <h1 class="text-4xl font-bold">404</h1>
    <h2>Not Found</h1>
        <p>@(!string.IsNullOrEmpty(Model) ? Model : "The item you are looking for does not exist or has been removed.")
        </p>
</div>